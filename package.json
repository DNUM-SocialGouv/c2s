{
  "name": "c2s_oc_front",
  "private": true,
  "version": "1.0.24",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx} c --coverageReporters=lcov --coverageReporters=html && open coverage/index.html",
    "test:coverage:ci": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx}",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "precommit": "yarn lint && yarn format:check && yarn test && yarn build"
  },
  "dependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@dataesr/react-dsfr": "^3.10.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.4.2",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@react-keycloak/web": "^3.4.0",
    "@tanstack/react-form": "^0.13.3",
    "@tinymce/tinymce-react": "^5.0.1",
    "@types/node": "^20.11.5",
    "@types/react-redux": "^7.1.33",
    "@types/redux-mock-store": "^1.0.6",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^24.0.2",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-hook-use-form": "^1.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^3.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.11",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/mocks/styleMock.js",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/mocks/fileMock.js",
      "^(\\.{1,2}/.*)\\.js$": "$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(string-width|strip-ansi|ansi-regex|test-json-import)/)"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "interfaces",
      "jestGlobalMocks.ts",
      "<rootDir>/src/main.tsx",
      "<rootDir>/src/store.tsx",
      "<rootDir>/src/utils/RolesList.ts",
      "<rootDir>/src/utils/RoutesList.ts",
      "<rootDir>/src/rootReducer.tsx",
      "<rootDir>/src/App.tsx",
      "<rootDir>/src/assets/",
      "<rootDir>/src/mocks/",
      "<rootDir>/src/keycloak/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50
      }
    },
    "setupFiles": [
      "./setupTests.js"
    ]
  }
}
